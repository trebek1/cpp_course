.PHONY: clean All

All:
	@echo "----------Building project:[ CopyConstructor - Debug ]----------"
	@cd "CopyConstructor" && "$(MAKE)" -f  "CopyConstructor.mk"
clean:
	@echo "----------Cleaning project:[ CopyConstructor - Debug ]----------"
	@cd "CopyConstructor" && "$(MAKE)" -f  "CopyConstructor.mk" clean
